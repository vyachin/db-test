{
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "yiisoft/yii2": "^2.0",
    "vlucas/phpdotenv": "^5.5"
  },
  "config": {
    "process-timeout": 1800,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "app\\": "."
    }
  }
}
